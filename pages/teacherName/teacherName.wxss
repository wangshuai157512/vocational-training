page {
    background:rgba(245,246,247,1);
}
.my_body {
    width: 750rpx;
    font-size:32rpx;
    font-family:PingFangSC-Regular,PingFang SC;
    font-weight:400;
    color:rgba(17,25,21,1); 
}
.my_bg {
    width: 100%;
    height: 380rpx;
}
.my_bg image{
    width: 100%;
    height: 100%;
}
.my_photo {
    width: 368rpx;
    color: #fff;
    position: absolute;
    left: 191rpx;
    top: 69rpx;
    text-align: center;
    display: flex;
    justify-content: center;
    flex-wrap: wrap;
}
.my_photo-btn {
    width: 100%;
}
.my_photo button{
    width: 100%;
    height: 100%;
    color: #fff;
    margin: 0;
    padding: 0;
    background: transparent;
    font-size:32rpx;
    font-family:PingFangSC-Regular,PingFang SC;
    font-weight:400;
}
.my_photo .my_photo_img {
    width: 150rpx;
    height: 150rpx;
    border-radius: 50%; 
    margin-bottom: 10rpx;
    position: relative; 
}
.my_photo image{
    width: 100%;
    height: 100%;
    border-radius: 50%;
    background-color: #ccc;
    
}
.my_photo .my_photo_img .avatarUrlBtn {
    width: 150rpx!important;
    height: 150rpx!important;
    border-radius: 50%!important;
    position: absolute; 
    top: 0;
    left: 0;
    padding: 0;
    margin: 0;
    background: transparent!important;
}
.my-content {
    position: absolute;
    left: 30rpx;
    top: 330rpx;
    width: 690rpx;
    padding: 50rpx 32rpx 50rpx 28rpx;
    box-sizing: border-box;
    background: #fff;
    border-radius:12rpx;
}
.page-section{width:690rpx;height:auto; background:#ffffff;border-radius:12rpx; position: absolute;left: 30rpx;top: 330rpx;}
.page-section .Title{width:100%;text-align: center;padding-top:30rpx;height:100rpx;box-sizing:border-box;display: block;line-height:80rpx; font-size:48rpx;overflow:hidden;font-family:PingFang-SC-Heavy,PingFang-SC;font-weight:800;color:rgba(24,25,26,1);position: relative;}

.page-section .formBlock{padding:40rpx;width:100%;box-sizing:border-box;}
.page-section .formBlock .section{margin:0rpx 0rpx 40rpx 0rpx;}
.page-section .formBlock .section .section__title{height:40rpx;width:100%;display:block; margin-bottom:10rpx;font-size:26rpx;font-family:PingFangSC-Regular,PingFang SC;font-weight:400;color:rgba(51,51,51,1);line-height:40rpx;}
.page-section .formBlock .section .section__input{height:88rpx;padding:0px 20rpx;width:100%;display:block; box-sizing:border-box;background:rgba(245,246,250,1);border-radius:6rpx;font-size:28rpx;color:rgba(51,51,51,1);}
.page-section .formBlock .btn-area{padding-top:0rpx;width:100%;}
.page-section .formBlock .btn-area button{width:100%;height:88rpx;background:linear-gradient(270deg,rgba(242,89,12,1) 0%,rgba(255,114,13,1) 100%);border-radius:6rpx;font-size:30rpx;font-family:PingFangSC-Medium,PingFang SC;font-weight:500;color:rgba(255,255,255,1);line-height:88rpx;padding:0rpx;}
.explain {
    width: 100%;
    height: 66rpx;
    line-height: 33rpx;
    font-size:24rpx;
    color:rgba(149,156,166,1);
    margin-bottom: 50rpx;
}
.lookMyshare {
    width:100%;
    height:42rpx;
    font-size:30rpx;
    font-family:PingFangSC-Regular,PingFang SC;
    font-weight:400;
    color:rgba(0,128,106,1);
    line-height:42rpx; 
    text-align: center;
    margin-top: 40rpx;
}