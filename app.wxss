/**app.wxss**/
.container {height: 100%;display: flex;flex-direction: column;justify-content: space-between;padding:0rpx;box-sizing: border-box;padding-bottom: 100rpx;background:rgba(245,246,250,1); }

.clearfix:after {content: '.';display: block;height: 0;clear: both;visibility: hidden;}
.clearfix .clear {_height: 0px;}

.weChat{position:fixed;right:25rpx;bottom:150rpx;border:0;width:100rpx;height:100rpx;display:block;}
.weChat button{border:0;width:100rpx;height:100rpx;text-align:left;padding:0;margin:0;border-radius:50%; display:block;line-height:0;float:left;background:rgba(255,0,0,0);}
.weChat button image{width:100rpx;height:100rpx;}
