page {
    background:rgba(245,246,247,1);
}
.my_body {
    width: 750rpx;
    font-size:32rpx;
    font-family:PingFangSC-Regular,PingFang SC;
    font-weight:400;
    color:rgba(17,25,21,1); 
}
.my_bg {
    width: 100%;
    height: 380rpx;
}
.my_bg image{
    width: 100%;
    height: 100%;
}
.my_photo {
    width: 368rpx;
    color: #fff;
    position: absolute;
    left: 191rpx;
    top: 69rpx;
    text-align: center;
    display: flex;
    justify-content: center;
    flex-wrap: wrap;
}
.my_photo-btn {
    width: 100%;
}
.my_photo button{
    width: 100%;
    height: 100%;
    color: #fff;
    margin: 0;
    padding: 0;
    background: transparent;
    font-size:32rpx;
    font-family:PingFangSC-Regular,PingFang SC;
    font-weight:400;
}
.my_photo .my_photo_img {
    width: 150rpx;
    height: 150rpx;
    border-radius: 50%; 
    margin-bottom: 10rpx;
    position: relative; 
}
.my_photo image{
    width: 100%;
    height: 100%;
    border-radius: 50%;
    background-color: #ccc;
    
}
.my_photo .my_photo_img .avatarUrlBtn {
    width: 150rpx!important;
    height: 150rpx!important;
    border-radius: 50%!important;
    position: absolute; 
    top: 0;
    left: 0;
    padding: 0;
    margin: 0;
    background: transparent!important;
}
.my-content {
    position: absolute;
    left: 30rpx;
    top: 330rpx;
    width: 690rpx;
    padding: 50rpx 32rpx 50rpx 28rpx;
    box-sizing: border-box;
    background: #fff;
    border-radius:8rpx;
}
.my-content > view {
    display: flex;
    justify-content: space-between;
    align-items: center;
    width: 100%;
    height: 48rpx;
    position: relative; 
}
.my-content > view button{
    width: 100%!important;
    height: 48rpx!important;
    position: absolute; 
    top: 0;
    left: 0;
    padding: 0;
    margin: 0;
    background: transparent;
}
.my-content > view view:first-child image{
    width: 48rpx;
    height: 48rpx;
    vertical-align: middle;
    margin-right: 8rpx;
}
.arrow {
    width: 24rpx;
    height: 24rpx;
}
.arrow image{
    width: 100%;
    height: 100%;
    vertical-align: top;
}
